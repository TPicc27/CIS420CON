    @{
        ViewBag.Title = "Home";
    }
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>School of Nursing - @ViewBag.Title</title>
        @Styles.Render("~/Content/css")
